{
  "name": "react-typescript-component-boilerplate",
  "version": "2.1.0",
  "description": "Boilerplate for creating reusable react component",
  "keywords": [
    "react",
    "typescript",
    "component",
    "boilerplate",
    "react typescript component"
  ],
  "author": "Alireza",
  "license": "MIT",
  "main": "lib/index.js",
  "module": "es/index.js",
  "scripts": {
    "start": "webpack-dev-server -d --debug --devtool  source-map  --output-pathinfo --progress --colors --port 3000 --open --host 0.0.0.0",
    "build:commonjs": "tsc",
    "build:es": "tsc -m esNext --outDir es",
    "build": "npm run build:commonjs && npm run build:es",
    "clean": "rimraf lib dist es coverage",
    "prepare": "npm run clean && npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "peerDependencies": {
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.6.0",
    "@babel/plugin-proposal-function-bind": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.6.0",
    "core-js": "^3.2.1",
    "@types/react": "^16.9.3",
    "@types/react-dom": "^16.9.1",
    "babel-loader": "^8.3.0",
    "css-loader": "^6.7.3",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.7.2",
    "rimraf": "^3.0.0",
    "style-loader": "^1.3.0",
    "ts-loader": "^9.4.2",
    "typescript": "^3.6.3",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^3.1.9"
  }
}
